<settings>
    <servers>
        <server>
            <id>ossrh</id>
            <username>${MAVEN_JIRA_USERNAME}</username>
            <password>${MAVEN_JIRA_PASSWORD}</password>
        </server>
    </servers>    
    <profiles>
        <profile>
            <id>ossrh</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <gpg.executable>gpg</gpg.executable>
                <gpg.passphrase>${SIGN_KEY_PASS}</gpg.passphrase>
            </properties>
        </profile>
    </profiles>
</settings>